



<table width="100%" style="border-collapse:collapse;border-spacing:0px;table-layout:fixed;" cellpadding="0" cellspacing="0" border="0" align="center">
  <tbody>
  <tr valign="top">
    <td>
      <table width="600" class="device-width" align="center" style="background-color:#FFF;border:1px solid #BCB5B9;margin:0px auto 0px auto;" cellpadding="0" cellspacing="0" border="0">
        <tbody>

        <tr>
          <td class="table-body-outer" style="padding:0px 0px 70px 0px;border-bottom:2px solid #C900D7;">

            <table class="table-body" width="100%" cellpadding="0" cellspacing="0" border="0" align="center" style="font-family: 'Montserrat', sans-serif;">
              <!--HEADER-->
              <tbody><tr>
                <td class="header-outer" align="center" style="padding:10px 20px 5px 20px;">

                  <table class="header" width="100%" cellpadding="0" cellspacing="0" border="0" align="center">
                    <tbody>
                    <tr>
                      <td>

                        <table width="100%" cellpadding="0" cellspacing="0" border="0">
                          <tbody>
                          <tr>
                            <td class="logo" style="font-family: 'bariolregular';font-size:30px;color:#bd10e0;font-weight:bold;text-align:right;padding:0px 0px 10px 0px;">
                              <a href="www.sobabooks.com" style="text-decoration:none;color:#bd10e0;">Sobabooks</a>
                            </td>
                          </tr>
                          </tbody>
                        </table>

                      </td>
                    </tr>

                    </tbody>
                  </table>

                </td>
              </tr>
              <!--BANNER-->


              <!-- CONTAINER -->
              <tr>
                <td class="container-block" style="padding:0 65px 0 65px;">

                  <table class="container" width="100%" cellpadding="0" cellspacing="0" border="0">
                    <!--WELCOME-BLOCK-->
                    <tbody><tr>
                      <td style="padding:0px 0px 22px 0px;">

                        <table class="welcome-block" width="100%" cellpadding="0" cellspacing="0" border="0">
                          <tbody>

                          <tr>
                            <td style="padding:0px 0px 30px 0px;" align="center">
                              <table width="100%" cellpadding="0" cellspacing="0" border="0">
                                <tbody>
                                <tr>
                                  <td class="user-title" style="color:#000000;font-size:18px;font-weight:500;border-bottom:1px solid #F1F1F1; padding-bottom: 2%" align="center">
                                    Thank you contacting Sobabooks Support!
                                  </td>
                                </tr>
                                <tr></tr>
                                <tr>
                                  <td class="user-title" style="color:#000000;font-size:18px;font-weight:500;border-bottom:1px solid #F1F1F1;" align="center">
                                    This is to confirm we've received your request. Our support Team will respond as soon as possibile.
                                  </td>
                                </tr>
                                </tbody>
                              </table>
                            </td>
                          </tr>

                          <tr>
                            <td style="padding:9px 0px 9px 0px;border-bottom:1px solid #F1F1F1;" align="center">
                              <table width="100%" cellpadding="0" cellspacing="0" border="0">
                                <tbody>
                                <tr>
                                  <td class="left-details" width="50%" style="color:#4d4d4d;font-size:12px;font-weight:500;" align="left">
                                    Name:
                                  </td>
                                  <td class="right-details" width="50%" style="color:#000000;font-size:12px;font-weight:500;" align="right">
                                    <%= @data.name %>
                                  </td>
                                </tr>
                                </tbody>
                              </table>
                            </td>
                          </tr>

                          <tr>
                            <td style="padding:9px 0px 9px 0px;border-bottom:1px solid #F1F1F1;" align="center">
                              <table width="100%" cellpadding="0" cellspacing="0" border="0">
                                <tbody>
                                <tr>
                                  <td class="left-details" width="50%" style="color:#4d4d4d;font-size:12px;font-weight:500;" align="left">
                                    E-mail:
                                  </td>
                                  <td class="right-details" width="50%" style="color:#000000;font-size:12px;font-weight:500;" align="right">
                                    <%= @data.email %>
                                  </td>
                                </tr>
                                </tbody>
                              </table>
                            </td>
                          </tr>
                          <tr>
                            <td style="padding:9px 0px 9px 0px;border-bottom:1px solid #F1F1F1;" align="center">
                              <table width="100%" cellpadding="0" cellspacing="0" border="0">
                                <tbody>
                                <tr>
                                  <td class="left-details" width="50%" style="color:#4d4d4d;font-size:12px;font-weight:500;" align="left">
                                    Phone:
                                  </td>
                                  <td class="right-details" width="50%" style="color:#000000;font-size:12px;font-weight:500;" align="right">
                                    <%= @data.phone %>
                                  </td>
                                </tr>
                                </tbody>
                              </table>
                            </td>
                          </tr>
                          <tr>
                            <td style="padding:9px 0px 9px 0px;border-bottom:1px solid #F1F1F1;" align="center">
                              <table width="100%" cellpadding="0" cellspacing="0" border="0">
                                <tbody>
                                <tr>
                                  <td class="left-details" width="50%" style="color:#4d4d4d;font-size:12px;font-weight:500;" align="left">
                                    Message:
                                  </td>
                                  <td class="right-details" width="50%" style="color:#000000;font-size:12px;font-weight:500;" align="right">
                                    <%= @data.message %>

                                  </td>
                                </tr>
                                </tbody>
                              </table>
                            </td>
                          </tr>

                          </tbody>
                        </table>

                      </td>
                    </tr>

                    </tbody></table>

                </td>
              </tr>
              </tbody></table>

          </td>
        </tr>

        </tbody>
      </table>
    </td>
  </tr>
  </tbody>
</table>


