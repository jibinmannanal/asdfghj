
<div class="col-md-4 col-sm-6 portfolio-item">



    <%=link_to image_tag("image.png", :alt => "", :class => "img-fluid") , answer.pdf.url, target: '_blank' %>



  <div class="portfolio-caption">
    <h4><%=answer.title%></h4>
    <p class="text-muted"><%=answer.status%></p>
    <p><%= link_to 'Show', answer %></p>
    <p><%= link_to 'Edit', edit_answer_path(answer) %></p>
    <td><%= link_to 'Destroy', answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </div>

</div>



